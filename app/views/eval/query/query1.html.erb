<div class='container'><table class="table">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">User</th>
    </tr>
  </thead>
  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td> <%= question.title %></td>
        <td> <%= question.user.name %></td>
      </tr>
    <% end%>    </tbody>
</table>
<%= button_to 'index', eval_queries_path, method: :get %>
</div>
